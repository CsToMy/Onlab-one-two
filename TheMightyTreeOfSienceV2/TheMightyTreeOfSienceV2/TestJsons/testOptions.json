 {
  "physics": {
    "barnesHut": {
      "centralGravity": 0.001
    },
    "minVelocity": 0.75
  },
  "interaction": {
    "hover": true,
    "hoverConnectedEdges": true,
    "keyboard": false
  },
  "edges": {
    "smooth": {
      "type": "discrete"
    },
    "width": 0.4,
    "selectionWidth": 0.5,
    "hoverWidth": 0.5,
    "arrows": { "to": true }
  },

  "nodes": {
    "shape": "text",
    "color": "#822309",
    "scaling": {
      "customScalingFunction": "function",
      "(min",
      "max",
      "total",
      "value)" {
        "if" "(value",
        "==" 0,
        "||" "value",
        ">=" "10)",
        {
          "return" "0.08;"
        },
        "else" "if",
        "(value" "==",
        "1)" {
          "return" "1;"
        },
        "else" "if",
        "(value" "==",
        "2)" {
          "return" "0.7;"
        },
        "else" "if",
        "(value" "==",
        "3)" {
          "return" "0.5;"
        },
        "else" "if",
        "(value" "==",
        "4)" {
          "return" "0.2;"
        },
        "else" "if",
        "(value" "==",
        "5)" {
          "return" "0.13;"
        },
        "else" "if",
        "(value" "==",
        "6)" {
          "return" "0.11;"
        },
        "else" "if",
        "(value" "==",
        "7)" {
          "return" "0.09;"
        },
        "else" "if",
        "(value" "==",
        "8)" {
          "return" "0.07;"
        },
        "else" "if",
        "(value" "==",
        "9)" {
          "return" "0.06;"
        }
      },
      "min": 0,
      "max": 10,
      "label": {
        "enabled": true,
        "min": 0,
        "max": 80,
        "maxVisible": 40,
        "drawThreshold": 20
      }
    }
  }
}
